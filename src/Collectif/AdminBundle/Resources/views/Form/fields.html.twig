{% block field_row %}
<div class="control-group">
    {{ form_label(form, label|default(null), { 'label_attr': { 'class': 'control-label' } }) }}   
    <div class="controls">
      {{ form_widget(form) }}
    </div>
	<div class="warning error">
		{{ form_errors(form) }}
	</div>
</div>
{% endblock field_row %}