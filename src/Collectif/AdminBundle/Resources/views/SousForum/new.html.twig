{% extends 'CollectifAdminBundle::layout.html.twig' %}

{% block title %}Collectif CONFLUENCE{% endblock %}

{% block content %}

<h1>Creation d'un sous forum</h1>

{% form_theme form 'CollectifAdminBundle:Form:fields.html.twig' %}

<form action="{{ path('reseau_sousforum_create', { 'type': type }) }}" method="post" {{ form_enctype(form) }} class="form-horizontal">
    {{ form_widget(form) }}
    
    <a class="btn btn-warning" href="{{ path('reseau_sousforum') }}">Retour</a>
    <input type="submit" class="btn btn-success" />
</form>

{% endblock %}