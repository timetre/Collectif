{% extends 'CollectifAdminBundle::layout.html.twig' %}

{% block title %}Collectif CONFLUENCE{% endblock %}

{% block content %}

<h1>Editer mes activités numériques</h1>

{% if message %}
<p>{{ message }}</p>
{% endif %}
{% form_theme form 'CollectifAdminBundle:Form:fields.html.twig' %}
<form action="" method="post" {{ form_enctype(form) }} class="form-horizontal" novalidate>
    {{ form_widget(form) }}
	
	<a class="btn btn-warning" href="{{ path('collectif_cv_list') }}">Retour</a>
	
    <input type="submit" class="btn btn-success" />
</form>

{% endblock %}

