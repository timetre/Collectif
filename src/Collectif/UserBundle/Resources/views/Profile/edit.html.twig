{% extends "CollectifAdminBundle::layout.html.twig" %}

{% block title %}Collectif CONFLUENCE{% endblock %}

{% block content %}

{% form_theme form 'CollectifAdminBundle:Form:fields.html.twig' %}
<form action="{{ path('fos_user_profile_edit') }}" {{ form_enctype(form) }} method="POST" class="form-horizontal">
    {{ form_widget(form) }}
    
    <input type="button" class="btn btn-warning" onclick="window.open('{{ path('collectif_membre_homepage') }}', '_parent');" value="Annuler" />
	
    <input type="submit" class="btn btn-success" value="{{ 'profile.edit.submit'|trans({}, 'FOSUserBundle') }}" />

</form>

{% endblock content %}
